{
  "name": "angular-modal-gallery",
  "version": "3.1.1",
  "description": "Modal image gallery for Angular >=2",
  "main": "index.ts",
  "license": "MIT",
  "author": "Stefano Cappa",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "homepage": "https://github.com/Ks89/angular-modal-gallery#readme",
  "scripts": {
    "clean": "rimraf dist aot bundle",
    "clean:all": "npm run clean && rimraf demo/systemjs/node_modules/angular-modal-gallery demo/webpack/node_modules/angular-modal-gallery rimraf demo/angular-cli/node_modules/angular-modal-gallery",
    "copy": "copyfiles src/**/*.scss dist && copyfiles src/**/*.html dist",
    "ngc": "ngc",
    "rollup": "rollup -c",
    "minify": "uglifyjs dist/bundles/angular-modal-gallery.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/angular-modal-gallery.umd.min.js",
    "prebuild": "npm run clean:all",
    "build": "npm run ngc && npm run copy && npm run rollup && npm run minify",
    "postbuild": "npm run bundle && npm run copy:demo:systemjs && npm run copy:demo:angularcli && npm run copy:demo:webpack",
    "bundle": "mkdirp bundle && ncp dist bundle/dist && ncp aot bundle/aot && ncp src bundle/src && copyfiles LICENSE README.md package.json index.ts index.d.ts bundle",
    "copy:demo:systemjs": "mkdirp demo/systemjs/node_modules/angular-modal-gallery && ncp bundle demo/systemjs/node_modules/angular-modal-gallery",
    "copy:demo:angularcli": "mkdirp demo/angular-cli/node_modules/angular-modal-gallery && ncp bundle demo/angular-cli/node_modules/angular-modal-gallery",
    "copy:demo:webpack": "mkdirp demo/webpack/node_modules/angular-modal-gallery && ncp bundle demo/webpack/node_modules/angular-modal-gallery",
    "clean-coverage": "rimraf coverage",
    "pretest": "npm run clean-coverage",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "docs": "typedoc --out docs src",
    "coveralls": "coveralls < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "peerDependencies": {
    "@angular/common": ">=2.0.0",
    "@angular/core": ">=2.0.0",
    "font-awesome": ">=4.0.0",
    "hammerjs": ">=2.0.0",
    "mousetrap": ">=1.6.1",
    "rxjs": ">=5.0.0"
  },
